{
  "presets": [
    ["@babel/preset-env", { "modules": false }],
    ["@babel/preset-react", { "runtime": "automatic" }]
  ],
  "plugins": [
    ["file-loader", {
      "name": "[name].[ext]",
      "extensions": ["css"]
    }],
    ["babel-plugin-transform-assets", {
      "extensions": ["css"]
    }]
  ]
}
